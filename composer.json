{
  "name": "gdbots/iam-bundle",
  "homepage": "https://github.com/gdbots/iam-bundle-php",
  "description": "Symfony3 bundle that integrates gdbots/iam library.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/iam": "dev-master as 0.1.0",
    "gdbots/ncr-bundle": "~0.1"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~6.0",
    "auth0/auth0-php": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\IamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\IamBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
