{
  "name": "gdbots/iam-bundle",
  "homepage": "https://github.com/gdbots/iam-bundle-php",
  "description": "Symfony bundle that provides security implementations for the gdbots/iam library.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/iam": "^1.0",
    "gdbots/ncr-bundle": "^1.0",
    "symfony/security": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "auth0/auth0-php": "^5.0",
    "gdbots/acme-schemas": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\IamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\IamBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
