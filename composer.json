{
  "name": "gdbots/iam-bundle",
  "homepage": "https://github.com/gdbots/iam-bundle-php",
  "description": "Symfony bundle that provides security implementations for the gdbots/iam library.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.5",
    "gdbots/iam": "^3.0",
    "gdbots/ncr-bundle": "^5.0",
    "symfony/security-bundle": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "auth0/auth0-php": "^8.18",
    "gdbots/acme-schemas": "^3.0",
    "nyholm/psr7": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\IamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\IamBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
