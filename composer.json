{
  "name": "gdbots/iam-bundle",
  "homepage": "https://github.com/gdbots/iam-bundle-php",
  "description": "Symfony bundle that provides security implementations for the gdbots/iam library.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "gdbots/iam": "2.x-dev",
    "gdbots/ncr-bundle": "^2.0",
    "symfony/security-core": "^5.1",
    "symfony/security-http": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "auth0/auth0-php": "^7.0",
    "gdbots/acme-schemas": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\IamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\IamBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
