{
  "name": "gdbots/iam-bundle",
  "homepage": "https://github.com/gdbots/iam-bundle-php",
  "description": "Symfony bundle that integrates gdbots/iam library.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/iam": "~0.1",
    "gdbots/ncr-bundle": "~0.2",
    "symfony/security": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "auth0/auth0-php": "~5.0",
    "gdbots/acme-schemas": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\IamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\IamBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
